{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/socket.io/.*",
        "dest": "https://your-backend-url.herokuapp.com",
        "headers": {
          "Access-Control-Allow-Origin": "*",
          "Access-Control-Allow-Methods": "GET,POST"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "headers": {
          "Access-Control-Allow-Origin": "*"
        }
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }